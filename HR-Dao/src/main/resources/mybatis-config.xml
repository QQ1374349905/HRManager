<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="logImpl" value="STDOUT_LOGGING"/>
        <setting name="cacheEnabled" value="false"/>
        <setting name="lazyLoadingEnabled" value="true"/>
        <setting name="aggressiveLazyLoading" value="false"/>
    </settings>

    <mappers>
        <mapper resource="mappers/UserMapper.xml"/>
        <mapper resource="mappers/IdentityMapper.xml"/>
        <mapper resource="mappers/Config_file_First_kindMapper.xml"/>
        <mapper resource="mappers/Config_file_second_KindMapper.xml"/>
        <mapper resource="mappers/Config_file_third_KindMapper.xml"/>
    </mappers>
</configuration>