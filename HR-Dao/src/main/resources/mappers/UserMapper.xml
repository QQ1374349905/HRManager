<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qnck.dao.login.IUserDao">
    <resultMap id="userMap" type="com.qnck.entity.User">
        <result column="user_id" property="userId"/>
        <result column="user_name" property="userName"/>
        <result column="user_true_name" property="userTrueName"/>
        <result column="user_password" property="userPassword"/>
        <association property="rights" select="com.qnck.dao.power.IrightsDao.queryAndcontro" column="r_id"/>
    </resultMap>
    <resultMap id="userRightsMap" type="com.qnck.entity.User">
        <result column="user_id" property="userId"/>
        <result column="user_name" property="userName"/>
        <result column="user_true_name" property="userTrueName"/>
        <result column="user_password" property="userPassword"/>
        <result property="rights.r_id" column="r_id"/>
    </resultMap>
</mapper>